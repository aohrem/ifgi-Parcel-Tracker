<eeml xmlns="http://www.eeml.org/xsd/0.5.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="0.5.1" xsi:schemaLocation="http://www.eeml.org/xsd/0.5.1 http://www.eeml.org/xsd/0.5.1/0.5.1.xsd">
	<environment creator="https://cosm.com/users/hdr">
		<title>{title}</title>
		<private>true</private>
		<location disposition="mobile">
			<lat></lat>
			<lon></lon>
		</location>
		<data id="lat">
			<tag>latitude</tag>
		</data>
		<data id="lon">
			<tag>longitude</tag>
		</data>
		<data id="temp">
			<tag>temperature</tag>
		</data>
		<data id="hum">
			<tag>humidity</tag>
		</data>
		<data id="acc">
			<tag>acceleration</tag>
		</data>
		<data id="brig">
			<tag>brightness</tag>
		</data>
	</environment>
</eeml>